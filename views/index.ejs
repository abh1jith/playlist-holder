<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">
    <title>Abhijith Playlist Holder</title>
</head>
<body >
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <div class="container-fluid d-flex justify-content-center">
        <a class="navbar-brand"><code>ABHIJITH's PLAYLIST</code></a>
        
    </div></nav>
    <div class="container">
    <center>
        <br>
    <div>
        <div class="card text-center">
            <div class="card-header">
                <h4>Songs that are already in playlist:</h4>
            </div>
            <div class="card-body">
        
        <table class="table table-borderless">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Song Name</th>
                    <th scope="col">Added by User</th>
                    <th scope="col">Platform</th>
                </tr>
            </thead>
            <tbody>
                
                <% for(var i=0; i < songsList.length; i++){ %>
                    <tr>
                    <th scope="row"> <%= i + 1 %></th>
                    <td>
                        <a href=<%= songsList[i].link %> target="_blank">
                            <%= songsList[i].title %>
                        </a>
                    </td>
                    <td> 
                        <%= songsList[i].user %> 
                    </td>
                    
                    <td>
                    <% if(songsList[i].platform == "youtube"){ %>
                            <img src="https://upload.wikimedia.org/wikipedia/commons/e/e1/Logo_of_YouTube_%282015-2017%29.svg" width="70">
                    <% } %>
                    <% if(songsList[i].platform == "spotify"){ %>
                        <img src="https://storage.googleapis.com/pr-newsroom-wp/1/2018/11/Spotify_Logo_CMYK_Green.png" width="70">
                    <% } %>
                    <% if(songsList[i].platform == "apple"){ %>
                        <img src="https://upload.wikimedia.org/wikipedia/commons/2/2a/Apple_Music_logo.svg" width="70">
                    <% } %>
                    <% if(songsList[i].platform=="wynk" ){ %>
                        <img src="data:image/png;base64," width="120">
                    <% } %>
                    <% if(songsList[i].platform=="gaana" ){ %>
                        <img src="https://upload.wikimedia.org/wikipedia/commons/0/01/Gaana_logo.svg"
                            width="70">
                    <% } %>
                        <!-- <%= songsList[i].platform %> -->
                    </td>
                    </tr>
                    <% } %>
                
            </tbody>
        </table>
        </div>

</div>
<br>

    <div class="card text-center">
        <div class="card-header">
            <h4>Add a New song to the playlist</h4>
        </div>
        <div class="card-body">
    <form action="/add" method="post">
        <div class="input-group flex-nowrap">
            <span class="input-group-text" id="addon-wrapping">Enter the title of the song:</span>
            <input type="text" name="title" class="form-control" >
        </div>
        <div class="input-group flex-nowrap">
        <span class="input-group-text" id="addon-wrapping-link">Enter the link to the Song:</span>
        <input type="text" name="link" class="form-control" >

        </div>
        <div class="input-group flex-nowrap">
            <span class="input-group-text"   id="addon-wrapping-link">Choose the platform of the link:</span>
            <select class="form-select" name="platform">
                <option value="youtube">Youtube</option>
                <option value="spotify">Spotify</option>
                <option value="apple">Apple Music</option>
                <option value="wynk">Wynk Music</option>
                <option value="gaana">Gaana</option>

            </select>
        </div>
        <div class="input-group flex-nowrap">
            <span class="input-group-text"  id="addon-wrapping-link">Enter your name:</span>
            <input type="text" name="user" class="form-control">
        
        </div>
        <button type="submit" class="btn btn-primary">Add to Playlist</button>
    </form>
    </div>
    </div>
<br>
    <div class="card text-center ">
    <div class="card-header">
        <h4>Remove a song from the playlist</h4>
    </div>
    <div class="card-body">
        <form action="/remove" method="post">
            <div class="input-group flex-nowrap">
                <span class="input-group-text" id="addon-wrapping-link">Select the title of the song to remove:</span>
                <select class="form-select" name="removeTitle">
                    <% for(var i=0; i < songsList.length; i++){ %>
                        <option value= <%= songsList[i].title %>>
                            <%= songsList[i].title %>
                        </option>
                        <% } %>
                </select>
            </div>
            <button type="submit" class="btn btn-primary">Remove from Playlist</button>
        </form>
    </div>
    </div>

    </center>
    </div>
    
    <div class="container">
        <footer class="py-3 my-4">
            <ul class="nav justify-content-center border-bottom pb-3 mb-3">
                <li class="nav-item"><a href="#" class="nav-link px-2 text-body-secondary">Home</a></li>
                <li class="nav-item"><a href="https://abhijithdameruppala.netlify.app/" class="nav-link px-2 text-body-secondary"  target="_blank">Portfolio</a></li>
                <li class="nav-item"><a href="https://github.com/abh1jith" class="nav-link px-2 text-body-secondary"  target="_blank">Github</a></li>
                <li class="nav-item"><a href="https://www.linkedin.com/in/abhijithdameruppala/" class="nav-link px-2 text-body-secondary"  target="_blank">LinkedIn</a></li>
            </ul>
            <p class="text-center text-body-secondary">&copy; 2023 ABHIJITH DAMERUPPALA</p>
        </footer>
    </div>
</body>

</html>